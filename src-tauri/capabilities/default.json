{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": ["main"],
  "permissions": [
    {
      "identifier": "http:default",
      "allow": [
        { "url": "https://www.virustotal.com/api/v3/*" },
        { "url": "https:*:*" },
        { "url": "http://localhost:5173/*" }
      ]
    },

    "core:default",
    "store:default",
    "notification:default",
    "http:default"
  ]
}
